import java.util.*;
public class Somatorio{
	public static void main (String [] args) {
		int i=1,soma=0;
		System.out.println("A sequência do somatório de 1 até 100 é:");
		while (i<=100) {	
			soma = soma + i;
			System.out.println(soma);
			i=i+1;
		}
		System.out.println("-------------------------------");
		System.out.println("Programa encerrado;");
	}
}
